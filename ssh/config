Host nano
  Hostname nano.bnl.gov
  ProxyCommand ssh -CTqe none ssh.bnl.gov nc %h %p
Host github.com
  IdentityFile ~/.ssh/id_github
  ControlMaster no
Host *
  Compression yes
  # CompressionLevel 7
  AddressFamily inet
  Ciphers arcfour256,arcfour128,arcfour,blowfish-cbc,aes128-cbc,chacha20-poly1305@openssh.com
  PreferredAuthentications publickey,keyboard-interactive,password
  Protocol 2
  XAuthLocation /usr/bin/xauth
  ControlMaster auto
  ControlPath /tmp/%u/%r@%h:%p
  ControlPersist 60
  IdentityFile ~/.ssh/id_rsa
# PermitLocalCommand yes

