Host *
  Compression yes
  # CompressionLevel 7
  AddressFamily inet
  Ciphers arcfour256,arcfour128,arcfour,blowfish-cbc,chacha20-poly1305@openssh.com,aes128-cbc,aes128-ctr,aes192-ctr,aes256-ctr
  PreferredAuthentications publickey,keyboard-interactive,password
  Protocol 2
  XAuthLocation /usr/bin/xauth
  ControlMaster auto
  ControlPath /tmp/%u/%r@%h:%p
  ControlPersist 60
  IdentityFile ~/.ssh/id_rsa
