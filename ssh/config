Host pc
  ForwardX11 yes
  ForwardX11Trusted yes
  Hostname localhost
  HostKeyAlias pc
  IdentityFile ~/.ssh/id_pc
  Port 2319
Host kali
  ForwardX11 yes
  ForwardX11Trusted yes
  Hostname s.princeton.edu
  ForwardAgent yes
  RemoteForward 2319 localhost:22
Host github.com
  IdentityFile ~/.ssh/id_github
  ControlMaster no
Host *
  ForwardX11 no
  ForwardX11Trusted no
  Compression yes
  # CompressionLevel 7
  AddressFamily inet
  Ciphers arcfour256,arcfour128,arcfour,blowfish-cbc,aes128-cbc
  PreferredAuthentications publickey,keyboard-interactive,password
  Protocol 2
  XAuthLocation /usr/bin/xauth
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p
  ControlPersist 60
  IdentityFile ~/.ssh/id_rsa
# PermitLocalCommand yes

