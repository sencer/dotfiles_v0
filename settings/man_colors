export LESS_TERMCAP_mb=$'\E[01;31m'       # begin blinking
export LESS_TERMCAP_md=$'\E[01;38;5;74m'  # begin bold
export LESS_TERMCAP_me=$'\E[0m'           # end mode
export LESS_TERMCAP_se=$'\E[0m'           # end standout-mode
export LESS_TERMCAP_so=$(tput setaf 9)    # begin standout-mode - info box
export LESS_TERMCAP_ue=$'\E[0m'           # end underline
export LESS_TERMCAP_us=$'\E[04;38;5;146m' # begin underline
export LESS_TERMCAP_mr=$(tput rev) # Reverse
export LESS_TERMCAP_mh=$(tput dim) # Half Bright
export LESS_TERMCAP_ZN=$(tput ssubm) # Stat Subsecipt
export LESS_TERMCAP_ZV=$(tput rsubm) # End Subsecipt
export LESS_TERMCAP_ZO=$(tput ssupm) # Start Superscript
export LESS_TERMCAP_ZW=$(tput rsupm) # End Superscript
